<div class="span12">
	<table class="table table-striped table-hover">
		<thead>
			<tr>
				<th>Name</th>
				<th style="width: 10%">Rank</th>
				<th style="width: 10%">% White</th>
				<th style="width: 10%">% Hispanic</th>
				<th style="width: 10%">% Black</th>
				<th style="width: 10%">% Asian</th>
				<th style="width: 10%">% Native</th>
			</tr>
		</thead>
		<tbody>
			<% @names.each do |name| %>
				<tr>
					<td><%= name.body %></td>
					<td><%= name.rank %></td>
					<td><%= name.pctwhite %></td>
					<td><%= name.pcthispanic %></td>
					<td><%= name.pctblack %></td>
					<td><%= name.pctasian %></td>
					<td><%= name.pctnative %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>